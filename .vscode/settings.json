{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "nixEnvSelector.nixFile": "${workspaceRoot}/flake.nix",
  "prettier.semi": true,
  "editor.formatOnSave": true,
  "cSpell.words": [
    "Beingessner",
    "comptime",
    "Fastchess",
    "globby",
    "GOATED",
    "miette",
    "Nextjs",
    "raylib",
    "relpath",
    "serde",
    "Shiki",
    "Stockfish",
    "Twoslash",
    "typeof",
    "vals"
  ],
  "editor.quickSuggestions": {
    "strings": true
  },
  "cSpell.enableFiletypes": ["!markdown"],
  "grammarly.files.include": ["**/readme.md", "**/README.md", "**/*.txt", "data/blog/**/*.md"],
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "file"
    }
  ]
}
